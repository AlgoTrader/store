    },
    "integration": {
      "root": "examples/integration/",
      "projectType": "application",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/integration",
            "index": "examples/integration/src/index.html",
            "main": "examples/integration/src/main.ts",
            "polyfills": "examples/integration/src/polyfills.ts",
            "tsConfig": "examples/integration/tsconfig.app.json",
            "assets": [
              {
                "glob": "favicon.ico",
                "input": "examples/integration/src",
                "output": "/"
              },
              {
                "glob": "**/*",
                "input": "examples/integration/src/assets",
                "output": "/assets"
              }
            ],
            "styles": [
              {
                "input": "examples/integration/src/styles.scss"
              }
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "examples/integration/src/environments/environment.ts",
                  "with": "examples/integration/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "integration:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "integration:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "integration:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "examples/integration/src/test.ts",
            "polyfills": "examples/integration/src/polyfills.ts",
            "tsConfig": "examples/integration/tsconfig.spec.json",
            "karmaConfig": "examples/integration/karma.conf.js",
            "styles": [
              {
                "input": "examples/integration/styles.scss"
              }
            ],
            "scripts": [],
            "assets": [
              {
                "glob": "favicon.ico",
                "input": "examples/integration/src/",
                "output": "/"
              },
              {
                "glob": "**/*",
                "input": "examples/integration/src/assets",
                "output": "/assets"
              }
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "examples/integration/tsconfig.app.json",
              "examples/integration/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "integration-e2e": {
      "root": "examples/integration-e2e/",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "examples/integration-e2e/protractor.conf.js",
            "devServerTarget": "integration:serve"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "examples/integration-e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
